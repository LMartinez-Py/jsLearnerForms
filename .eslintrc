{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 1,
        "no-unused-vars": 1
    },
    "globals": {
        "require": false,
        "describe": false,
        "it": false,
        "beforeEach": false,
        "afterEach": false,
        "module": false,
        "console": false,
        "jfp": true,
        "signet": true,
        "global": false,
        "verify": false,
        "setTimeout": false,
        "Promise": false,
        "exploreFunction": false,
        "process": false,
        "__dirname": false
    }
}
